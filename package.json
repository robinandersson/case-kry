{
  "name": "case-kry",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "css:build": "postcss src/styles/main.css -o src/index.build.css",
    "css:watch": "postcss src/styles/main.css -o src/index.build.css -w",
    "js:start": "react-scripts start",
    "js:build": "react-scripts build",
    "build": "npm run css:build ; npm run js:build",
    "start": "npm-run-all -p -r css:watch js:start"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.6.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.2",
    "postcss-import": "^12.0.1",
    "tailwindcss": "^1.0.3"
  }
}
